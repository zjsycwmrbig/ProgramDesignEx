zjs = 15665777392

global s {
  '新东':return "我认识他,他是我舍友"
  
  <电话>:return zjs goto g
  default : return "听不懂"
}

g {
  '新东':return "我认识他,他是我舍友"
  <张靳生>:return "你傻逼?你不就是他"
  ["查询".sb]: return sb + "的电话是" + Query($tb="telephone",$id=sb)
  `.*浩.*诚`:return "123465635 支离破碎的老铁"
  default:return "其他电话"
}

z {
  wait 10:return "超时请求!"
}